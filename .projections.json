{
  "functions/*.c": {
    "alternate": "functions/{}.h",
    "type": "functions_source"
  },
  "functions/*.h": {
    "alternate": "functions/{}.c",
    "type": "functions_header"
  },
  "lisp/*.c": {
    "alternate": "lisp/{}.h",
    "type": "lisp_source"
  },
  "lisp/*.h": {
    "alternate": "lisp/{}.c",
    "type": "lisp_header"
  },
  "syntax/*.rl": {
    "alternate": "syntax/{}.h",
    "type": "syntax_source"
  },
  "syntax/*.h": {
    "alternate": "syntax/{}.rl",
    "type": "syntax_header"
  }
}
